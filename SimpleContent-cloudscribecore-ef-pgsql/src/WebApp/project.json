    "cloudscribe.SimpleContent.Storage.EFCore.pgsql": "1.1.0-*",
    "cloudscribe.SimpleContent.MetaWeblog": "1.1.0-*",
    "cloudscribe.SimpleContent.Web": "1.1.0-*",
    "cloudscribe.SimpleContent.Syndication": "1.1.0-*",
